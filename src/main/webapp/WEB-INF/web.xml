<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>RegisterMember</servlet-name>
		<display-name>RegisterMember</display-name>
		<description></description>
		<servlet-class>controller.RegisterMember</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetMembers</servlet-name>
		<display-name>GetMembers</display-name>
		<description></description>
		<servlet-class>controller.GetMembers</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterMember</servlet-name>
		<url-pattern>/RegisterMember</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetMembers</servlet-name>
		<url-pattern>/GetMembers</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>register.jsp</welcome-file>
	</welcome-file-list>
</web-app>
